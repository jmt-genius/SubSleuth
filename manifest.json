{
  "manifest_version": 3,
  "name": "SubSleuth",
  "version": "1.0",
  "permissions": [
    "tabs",
    "activeTab",
    "http://*/*",  
    "https://*/*"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "128": "logomain.png"
    }
  },
  "icons": {
    "128": "logomain.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "web_accessible_resources": ["content.js"],
  "background": {
    "service_worker": "background.js"
  }
}
